<h3> parts List </h3>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      
      <th scope="col">accountNumber</th>
      <th scope="col">totalService</th>
      <th scope="col">totalbill</th>
      <th scope="col">details</th>

    </tr>
  </thead>
  <tbody>
    <% accounts.forEach((account)=>{%>
    <tr>
      <td>
        <%=account.a_id%>
      </td>
      <td>
        <%=account.accountNumber%>
      </td>
      <td>
        <%=account.totalService%>
      </td>
      <td>
        <%=account.totalbill%>
      </td>

      <td>
          <%=account.ischecked%>
        </td>

      <td>
        <a class="btn btn-primary" href="/accounts/details/<%=account.a_id%>">Edit</a>

      </td>
    </tr>
    <%})%>
  </tbody>
</table>